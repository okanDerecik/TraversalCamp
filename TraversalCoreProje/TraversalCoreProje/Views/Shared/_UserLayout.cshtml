
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/MemberLayoutPartial/_HeaderPartial.cshtml") @*Header Bölümü Partial*@

<body class="g-sidenav-show  bg-gray-100">
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_NavbarPartial.cshtml") @*Navbar Bölümü Partial*@

    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
        <!-- Navbar -->
        @await Html.PartialAsync("/Views/MemberLayoutPartial/_Navbar2Partial.cshtml") @*Navbar 2 Bölümü Partial*@
        <!-- End Navbar -->
        <div class="container-fluid py-4">
            @RenderBody()
            @await Html.PartialAsync("/Views/MemberLayoutPartial/_FooterPartial.cshtml") @*Footer Bölümü Partial*@
        </div>
    </main>
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_RightbarPartial.cshtml") @*Rightbar kısmı Yani Açılır Kapanır Bölümü Partial*@
    <!--   Core JS Files   -->
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_ScriptPartial.cshtml") @*JavaScript Kodlarının Bulunduğu Bölüm Partial*@
</body>

</html>
